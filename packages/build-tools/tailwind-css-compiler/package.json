{
  "name": "@vjs-10/tailwind-css-compiler",
  "type": "module",
  "version": "1.0.0",
  "description": "Tailwind CSS to semantic CSS compiler for VJS-10",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "bin": {
    "tailwind-css-compiler": "dist/cli.js"
  },
  "scripts": {
    "build": "tsdown",
    "build:watch": "tsdown --watch --silent",
    "dev": "pnpm run build:watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --project tsconfig.typecheck.json",
    "clean": "rm -rf dist coverage"
  },
  "dependencies": {
    "@babel/parser": "^7.23.0",
    "@babel/traverse": "^7.23.0",
    "@babel/types": "^7.23.0",
    "@tailwindcss/container-queries": "^0.1.1",
    "@tailwindcss/postcss": "^4.1.13",
    "fs-extra": "^11.2.0",
    "glob": "^10.3.0",
    "postcss": "^8.4.0",
    "tailwindcss": "^4.1.13"
  },
  "devDependencies": {
    "@types/babel__traverse": "^7.20.0",
    "@types/fs-extra": "^11.0.0",
    "@types/node": "^20.0.0",
    "@vitest/coverage-v8": "^2.0.0",
    "tsdown": "^0.15.4",
    "tsx": "^4.0.0",
    "typescript": "^5.9.2",
    "vitest": "^2.0.0"
  },
  "keywords": [
    "tailwind",
    "css",
    "postcss",
    "vjs-10",
    "compiler"
  ],
  "author": "Video.js Contributors",
  "license": "Apache-2.0"
}
